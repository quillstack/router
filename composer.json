{
    "name": "quillstack/router",
    "description": "The routing library working with PSR-7 requests.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "router",
        "routing",
        "routes",
        "php",
        "php7"
    ],
    "homepage": "https://quillstack.org/router",
    "authors": [
        {
            "name": "Radek Ziemniewicz",
            "email": "radek@quillstack.org"
        }
    ],
    "require": {
        "php": "^8.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-message": "^1.0",
        "quillstack/http-request": "^0.0.1",
        "quillstack/server-request": "^0.0.2"
    },
    "autoload": {
        "psr-4": {
            "Quillstack\\Router\\": "src/",
            "Quillstack\\Router\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.x-dev"
        }
    },
    "require-dev": {
        "quillstack/unit-tests": "^0.2.9"
    }
}
